{
    "name": "habeebdev/twilio-webhook-tester",
    "description": "PHP wrapper for Twilio CLI webhook plugin - Test Twilio SMS webhooks locally using the Twilio CLI webhook plugin",
    "type": "library",
    "keywords": ["twilio", "webhook", "cli", "testing", "sms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Habeeb Dev",
            "email": "forhabeeb@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.24"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Twilio\\WebhookCli\\": "src/"
        }
    },
    "bin": [
        "bin/twilio-webhook"
    ],
    "minimum-stability": "stable",
    "prefer-stable": true
}
